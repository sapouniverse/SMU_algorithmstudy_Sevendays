   "source": [
    "a,b = map(int,input().split())\n",
    "def gcd(a,b):\n",
    "    mod = a % b\n",
    "    while mod >0:\n",
    "        a = b\n",
    "        b = mod\n",
    "        mod = a % b\n",
    "    return b\n",
    "\n",
    "def lcm(a, b):\n",
    "    return a * b // gcd(a,b)\n",
    "\n",
    "print(gcd(a, b))\n",
    "print(lcm(a, b))\n",
    "\n",
    "#Euclidean Algorithm"
   ]
  }
